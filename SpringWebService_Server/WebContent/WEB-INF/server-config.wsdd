  <ns1:parameter name="enableRemoteAdmin" value="false"/>
  <ns1:parameter name="className" value="org.apache.axis.utils.Admin"/>
  <ns1:namespace>http://xml.apache.org/axis/wsdd/</ns1:namespace>
 </ns1:service>
 <ns1:service name="Version" provider="java:RPC">
  <ns1:parameter name="allowedMethods" value="getVersion"/>
  <ns1:parameter name="className" value="org.apache.axis.Version"/>
 </ns1:service>
 <ns1:service name="ProductWS" provider="java:RPC" style="wrapped" use="literal">
  <ns1:operation name="find" qname="ns3:find" returnQName="ns3:findReturn" returnType="ns4:Product" soapAction="" xmlns:ns3="http://ws" xmlns:ns4="http://entities"/>
  <ns1:parameter name="allowedMethods" value="find"/>
  <ns1:parameter name="typeMappingVersion" value="1.2"/>
  <ns1:parameter name="wsdlPortType" value="ProductWS"/>
  <ns1:parameter name="className" value="ws.ProductWS"/>
  <ns1:parameter name="wsdlServicePort" value="ProductWS"/>
  <ns1:parameter name="schemaQualified" value="http://ws,http://entities"/>
  <ns1:parameter name="wsdlTargetNamespace" value="http://ws"/>
  <ns1:parameter name="wsdlServiceElement" value="ProductWSService"/>
  <ns1:typeMapping deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory" encodingStyle="" qname="ns5:Product" serializer="org.apache.axis.encoding.ser.BeanSerializerFactory" type="java:entities.Product" xmlns:ns5="http://entities"/>
