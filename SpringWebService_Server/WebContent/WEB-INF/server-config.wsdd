 <ns1:service name="HelloWS" provider="java:RPC" style="wrapped" use="literal">
  <ns2:operation name="sum" qname="ns1:sum" returnQName="ns1:sumReturn" returnType="xsd:int" soapAction="" xmlns:ns1="http://ws" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://xml.apache.org/axis/wsdd/">
   <ns2:parameter qname="ns1:a" type="xsd:int"/>
   <ns2:parameter qname="ns1:b" type="xsd:int"/>
  </ns2:operation>
  <ns1:operation name="hello" qname="ns2:hello" returnQName="ns2:helloReturn" returnType="xsd:string" soapAction="" xmlns:ns2="http://ws" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <ns1:parameter qname="ns2:name" type="xsd:string"/>
  </ns1:operation>
  <ns1:parameter name="allowedMethods" value="sum hello"/>
  <ns1:parameter name="typeMappingVersion" value="1.2"/>
  <ns1:parameter name="wsdlPortType" value="HelloWS"/>
  <ns1:parameter name="className" value="ws.HelloWS"/>
  <ns1:parameter name="wsdlServicePort" value="HelloWS"/>
  <ns1:parameter name="schemaQualified" value="http://ws"/>
  <ns1:parameter name="wsdlTargetNamespace" value="http://ws"/>
  <ns1:parameter name="wsdlServiceElement" value="HelloWSService"/>
